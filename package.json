{
	"name": "eteg",
	"version": "0.1.1",
	"description": "PoC: system for registering users",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev:webpack": "next dev --webpack",
		"build": "next build",
		"start": "next start",
		"lint": "eslint . --cache",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:quiet": "vitest run --reporter dot --silent",
		"test:coverage": "vitest run --coverage",
		"db:generate": "bun prisma generate",
		"db:migrate": "bun prisma migrate dev",
		"ci:migrate": "bun run db:generate && bun run db:migrate:deploy",
		"db:migrate:deploy": "bun prisma migrate deploy",
		"db:studio": "bun prisma studio",
		"docker-compose": "docker compose --project-directory . -f docker/docker-compose.yml",
		"docker-compose:prod": "docker compose --project-directory . -f docker/docker-compose.prod.yml",
		"docker:up": "bun run docker-compose -- up -d",
		"docker:up:build": "bun run docker-compose -- up --build -d",
		"docker:down": "bun run docker-compose -- down -v",
		"docker:logs": "bun run docker-compose -- logs -f",
		"docker:up:prod": "bun run docker-compose:prod -- up --build -d",
		"docker:down:prod": "bun run docker-compose:prod -- down -v",
		"docker:logs:prod": "bun run docker-compose:prod -- logs -f"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.2.2",
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "^7.2.0",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@t3-oss/env-nextjs": "^0.13.8",
		"@tailwindcss/postcss": "^4.1.13",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.7",
		"gerador-validador-cpf": "^6.3.0",
		"http-status-codes": "^2.3.0",
		"imask": "^7.6.1",
		"lucide-react": "^0.562.0",
		"next": "^16.1.0",
		"next-themes": "^0.4.6",
		"pg": "^8.16.3",
		"postcss": "^8.5.6",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-hook-form": "^7.70.0",
		"react-imask": "^7.6.1",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"prisma": "^7.2.0",
		"tailwindcss": "^4.1.13",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.1",
		"@next/eslint-plugin-next": "^16.1.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/bun": "^1.3.5",
		"@types/eslint": "^9.6.1",
		"@types/eslint-plugin-tailwindcss": "^3.17.0",
		"@types/node": "^24.6.0",
		"@types/pg": "^8.16.0",
		"@types/react-dom": "^19.1.9",
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/coverage-v8": "^4.0.17",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-tailwindcss": "^4.0.0-beta.0",
		"globals": "^16.0.0",
		"jsdom": "^27.4.0",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.4.2",
		"prettier-plugin-jsdoc": "^1.3.2",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"tsx": "^4.20.5",
		"tw-animate-css": "^1.4.0",
		"type-fest": "^4.37.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.20.0",
		"vite-tsconfig-paths": "^6.0.3",
		"vitest": "^4.0.16"
	}
}
